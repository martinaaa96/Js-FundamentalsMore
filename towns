function towns(array){

    let city = {}
  
    for( let el of array){


        let spliter = el.split(' | ')
        city.town = spliter[0]
        city.latitude = Number(spliter[1]).toFixed(2)
        city.longitude = Number(spliter[2]).toFixed(2)
        console.log(city)
    }

   

}
towns(['Sofia | 42.696552 | 23.32601',
'Beijing | 39.913818 | 116.363625']
)
